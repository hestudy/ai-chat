datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
 
generator client {
  provider = "prisma-client-js"
}
 
model User {
  id            String          @id @default(cuid())
  name          String?
  email         String         @unique
  image         String?
  password      String
 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}